  },
  "homepage": "https://bitbucket.org/alexanderwilliamson/bucks-poker.com#readme",
  "devDependencies": {
    "fs-extra": "^10.1.0",
    "glob-promise": "^5.0.0",
    "xlsx": "^0.18.5"
  }
}