{
  "name": "bucks-poker.com",
  "version": "1.0.0",
  "description": "Deploy scripts for bucks-poker.com",
  "scripts": {
    "preinstall": "cd app && npm install",
    "deploy:export": "cd app && npm run export",
    "deploy:sync": "aws s3 sync ./app/out/ s3://www.bucks-poker.com --delete --acl public-read",
    "deploy": "npm run deploy:export && npm run deploy:sync",
    "dev": "cd app && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/alexanderwilliamson/bucks-poker.com.git"
  },
